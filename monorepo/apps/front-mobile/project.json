{
  "sourceRoot": "apps/front-mobile/src",
  "projectType": "application",
  "targets": {
    "analyze": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/front-mobile"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "apps/front-mobile"
      }
    },
    "format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter --verbose format apps/front-mobile/lib/",
        "cwd": "apps/front-mobile"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "apps/front-mobile"
      }
    },
    "assemble": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter assemble",
        "cwd": "apps/front-mobile"
      }
    },
    "attach": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter attach",
        "cwd": "apps/front-mobile"
      }
    },
    "drive": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter drive",
        "cwd": "apps/front-mobile"
      }
    },
    "genL10n": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter gen-l10n",
        "cwd": "apps/front-mobile"
      }
    },
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter install",
        "cwd": "apps/front-mobile"
      }
    },
    "run": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter run -d chrome",
        "cwd": "apps/front-mobile"
      }
    },
    "buildAar": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "apps/front-mobile"
      }
    },
    "buildApk": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "apps/front-mobile"
      }
    },
    "buildAppbundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build appbundle",
        "cwd": "apps/front-mobile"
      }
    },
    "buildBundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "apps/front-mobile"
      }
    },
    "buildIos": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "apps/front-mobile"
      }
    },
    "buildIosFramework": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "apps/front-mobile"
      }
    },
    "buildIpa": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ipa",
        "cwd": "apps/front-mobile"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "postTargets": ["front-mobile:github"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    }
  },
  "tags": []
}
