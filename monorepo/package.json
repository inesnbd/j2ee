{
  "name": "projet-safeway",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "pnpm dlx husky-init; npx husky set .husky/pre-commit \"pnpm precommit\" && husky install",
    "start": "nx run-many --all --target=serve",
    "format": "nx run-many --all --target=format",
    "build": "nx run-many --all --target=build",
    "test": "nx run-many --all --target=test",
    "buildImage": "nx run-many --all --target=buildImage",
    "precommit": "pnpm precommit:format && pnpm precommit:test",
    "precommit:format": "pnpm nx affected --target format --head=HEAD --base=develop",
    "precommit:test": "pnpm nx affected --target test --head=HEAD --base=develop",
    "api:generate": "pnpm openapi-generator-cli generate"
  },
  "private": true,
  "devDependencies": {
